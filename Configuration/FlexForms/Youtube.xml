<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>YouTube: Playback</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <autoplay>
                        <TCEforms>
                            <label>Autoplay</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </autoplay>
                    <start>
                        <TCEforms>
                            <label>Start time (in seconds)</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </start>
                    <end>
                        <TCEforms>
                            <label>End time (in seconds)</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </end>
                    <loop>
                        <TCEforms>
                            <label>Play video in loop</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </loop>
                </el>
            </ROOT>
        </sDEF>
        <appearance>
            <ROOT>
                <TCEforms>
                    <sheetTitle>YouTube: Player UI</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <preventFullScreen>
                        <TCEforms>
                            <label>Prevent full screen</label>
                            <config>
                                <type>check</type>
                            </config>check
                        </TCEforms>
                    </preventFullScreen>
                    <cc_load_policy>
                        <TCEforms>
                            <label>Show captions by default</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </cc_load_policy>
                    <color>
                        <TCEforms>
                            <label>Color</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">red</numIndex>
                                        <numIndex index="1">red</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">white</numIndex>
                                        <numIndex index="1">white</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </color>
                    <hideControls>
                        <TCEforms>
                            <label>Hide Player controls</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </hideControls>
                    <hideAnnotation>
                        <TCEforms>
                            <label>Hide Video annotations</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </hideAnnotation>
                    <modestBranding>
                        <TCEforms>
                            <label>Less YouTube branding</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </modestBranding>
                    <relatedVideos>
                        <TCEforms>
                            <label>Show related videos after playback</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </relatedVideos>
                </el>
            </ROOT>
        </appearance>
        <advanced>
            <ROOT>
                <TCEforms>
                    <sheetTitle>YouTube: Advanced Features</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <enablejsapi>
                        <TCEforms>
                            <label>Enable JS API</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </enablejsapi>
                    <passLanguageAlong>
                        <TCEforms>
                            <label>Pass the frontend language to YouTube</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </passLanguageAlong>
                </el>
            </ROOT>
        </advanced>
    </sheets>
</T3DataStructure>
